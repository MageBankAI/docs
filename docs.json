{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "MageBank Docs",
    "colors": {
        "primary": "#FF5722",
        "light": "#FF7043",
        "dark": "#E64A19"
    },
    "favicon": "/favicon.ico",
    "navigation": {
        "tabs": [
            {
                "tab": "Guides",
                "groups": [
                    {
                        "group": "Get Started",
                        "pages": [
                            "index",
                            "quickstart",
                            "development"
                        ]
                    },
                    {
                        "group": "User Guide",
                        "pages": [
                            "user-guide/get-api-key",
                            "user-guide/register-agent",
                            "user-guide/transactions",
                            "user-guide/savings-vault"
                        ]
                    }
                ]
            },
            {
                "tab": "API Reference",
                "groups": [
                    {
                        "group": "API Documentation",
                        "pages": [
                            "api-reference/introduction"
                        ]
                    },
                    {
                        "group": "Endpoint Examples",
                        "pages": [
                            "api-reference/endpoint/get",
                            "api-reference/endpoint/create",
                            "api-reference/endpoint/delete",
                            "api-reference/endpoint/webhook"
                        ]
                    }
                ]
            }
        ],
        "global": {
            "anchors": [
                {
                    "anchor": "Documentation",
                    "href": "https://mintlify.com/docs",
                    "icon": "book-open-cover"
                },
                {
                    "anchor": "Community",
                    "href": "https://mintlify.com/community",
                    "icon": "slack"
                },
                {
                    "anchor": "Blog",
                    "href": "https://mintlify.com/blog",
                    "icon": "newspaper"
                }
            ]
        }
    },
    "logo": {
        "light": "/logo/logo.svg",
        "dark": "/logo/logo.svg"
    },
    "navbar": {
        "links": [
            {
                "label": "Support",
                "href": "mailto:support@magebank.ai"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Dashboard",
            "href": "https://www.magebank.ai/dashboard"
        }
    },
    "footer": {
        "socials": {
            "x": "https://x.com/magebank",
            "github": "https://github.com/magebank",
            "linkedin": "https://linkedin.com/company/magebank"
        }
    }
}